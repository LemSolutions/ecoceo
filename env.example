# Supabase Configuration
NEXT_PUBLIC_SUPABASE_URL=your_supabase_url_here
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_publishable_key_here
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key_here
SUPABASE_SECRET_KEY=your_supabase_secret_key_here

# AI Integrations
# OpenAI
NEXT_PUBLIC_OPENAI_API_KEY=sk-your_openai_api_key_here

# Google AI
NEXT_PUBLIC_GOOGLE_AI_API_KEY=AIza_your_google_ai_api_key_here

# Google PageSpeed Insights API (for performance analysis)
# Get your API key: https://console.cloud.google.com/apis/credentials
# Enable PageSpeed Insights API: https://console.cloud.google.com/apis/library/pagespeedonline.googleapis.com
GOOGLE_PAGESPEED_API_KEY=your_google_pagespeed_api_key_here

# Anthropic
NEXT_PUBLIC_ANTHROPIC_API_KEY=sk-ant-your_anthropic_api_key_here

# Midjourney (se disponibile)
NEXT_PUBLIC_MIDJOURNEY_API_KEY=your_midjourney_api_key_here

# GitHub Copilot (se disponibile)
NEXT_PUBLIC_GITHUB_COPILOT_TOKEN=your_github_copilot_token_here

# Custom AI APIs
NEXT_PUBLIC_CUSTOM_AI_API_KEY=your_custom_ai_api_key_here
NEXT_PUBLIC_CUSTOM_AI_BASE_URL=https://your-custom-ai-api.com

# Sanity CMS (se utilizzato)
NEXT_PUBLIC_SANITY_PROJECT_ID=your_sanity_project_id
NEXT_PUBLIC_SANITY_DATASET=production
NEXT_PUBLIC_SANITY_API_VERSION=2023-05-03
SANITY_API_TOKEN=your_sanity_api_token_here

# Password Protection
# Area Clienti (server-side password)
AREA_CLIENTI_PASSWORD=your_client_area_password_here

# Dashboard (client-side password - NEXT_PUBLIC prefix for browser access)
NEXT_PUBLIC_DASHBOARD_PASSWORD=your_dashboard_password_here

# Stripe Configuration
STRIPE_SECRET_KEY=sk_test_your_stripe_secret_key_here
NEXT_PUBLIC_STRIPE_PUBLISHABLE_KEY=pk_test_your_stripe_publishable_key_here
STRIPE_WEBHOOK_SECRET=whsec_your_webhook_secret_here

# Other configurations
NEXT_PUBLIC_APP_URL=http://localhost:3000
NEXT_PUBLIC_APP_NAME=Dashboard Aziendale
